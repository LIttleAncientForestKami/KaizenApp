<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<AnchorPane prefHeight="600.0" prefWidth="600.0"
	style="-fx-background-color: radial-gradient( focus-angle 0.0deg, focus-distance 0.0%, center 50.0% 50.0%, radius 100.0%, rgb(204.0,255.0,255.0) 0.0, rgb(128.0,153.0,255.0) 100.0 );"
	xmlns="http://javafx.com/javafx/8.0.92" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="com.agh.mbulawa.view.AdminLayoutController">
	<children>
		<BorderPane prefHeight="600.0" prefWidth="600.0"
			style="-fx-background-color: radial-gradient( focus-angle 0.0deg, focus-distance 0.0%, center 50.0% 50.0%, radius 100.0%, rgb(204.0,255.0,255.0) 0.0, rgb(128.0,153.0,255.0) 100.0 );"
			AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
			AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
			<center>
				<TabPane prefHeight="200.0" prefWidth="200.0" side="LEFT"
					style="-fx-background-color: radial-gradient( focus-angle 0.0deg, focus-distance 0.0%, center 50.0% 50.0%, radius 100.0%, rgb(204.0,255.0,255.0) 0.0, rgb(128.0,153.0,255.0) 100.0 )( focus-angle 0.0deg, focus-distance 0.0%, center 50.0% 50.0%, radius 100.0%, rgb(204.0,255.0,255.0) 0.0, rgb(128.0,153.0,255.0) 100.0 );"
					tabClosingPolicy="UNAVAILABLE" BorderPane.alignment="CENTER">
					<tabs>
						<Tab
							style="-fx-background-color: radial-gradient( focus-angle 0.0deg, focus-distance 0.0%, center 50.0% 50.0%, radius 100.0%, rgb(179,230,230) 0.0, rgb(0,26,128) 100.0 );"
							text="ListaPracowników">
							<content>
								<AnchorPane
									style="-fx-background-color: radial-gradient( focus-angle 0.0deg, focus-distance 0.0%, center 50.0% 50.0%, radius 100.0%, rgb(204.0,255.0,255.0) 0.0, rgb(128.0,153.0,255.0) 100.0 );">
									<children>
										<VBox alignment="CENTER" prefHeight="575.0" prefWidth="571.0"
											AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
											AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
											<children>
												<HBox alignment="CENTER" spacing="10.0">
													<children>
														<Button fx:id="showIdeasButton" mnemonicParsing="false"
															onAction="#handleShowUserIdeas"
															style="-fx-background-color: radial-gradient( focus-angle 0.0deg, focus-distance 0.0%, center 50.0% 50.0%, radius 100.0%, rgb(179,230,230) 0.0, rgb(0,26,128) 100.0 );"
															text="Wyświetl Pomysły Pracownika">
															<font>
																<Font name="System Bold" size="12.0" />
															</font>
														</Button>
														<Button mnemonicParsing="false" onAction="#handleAddUser"
															style="-fx-background-color: radial-gradient( focus-angle 0.0deg, focus-distance 0.0%, center 50.0% 50.0%, radius 100.0%, rgb(179,230,230) 0.0, rgb(0,26,128) 100.0 );"
															text="Dodaj Pracownika">
															<font>
																<Font name="System Bold" size="12.0" />
															</font>
														</Button>
														<Button fx:id="editUserButton" mnemonicParsing="false"
															onAction="#handlaEditUser"
															style="-fx-background-color: radial-gradient( focus-angle 0.0deg, focus-distance 0.0%, center 50.0% 50.0%, radius 100.0%, rgb(179,230,230) 0.0, rgb(0,26,128) 100.0 );"
															text="Edytuj Pracownika">
															<font>
																<Font name="System Bold" size="12.0" />
															</font>
														</Button>
														<Button fx:id="delUserButton" mnemonicParsing="false"
															onAction="#handleRemoveUser"
															style="-fx-background-color: radial-gradient( focus-angle 0.0deg, focus-distance 0.0%, center 50.0% 50.0%, radius 100.0%, rgb(179,230,230) 0.0, rgb(0,26,128) 100.0 );"
															text="Usuń Pracownika">
															<font>
																<Font name="System Bold" size="12.0" />
															</font>
														</Button>
													</children>
													<VBox.margin>
														<Insets bottom="5.0" />
													</VBox.margin>
												</HBox>
												<TableView fx:id="usersTable" VBox.vgrow="ALWAYS">
													<columns>
														<TableColumn fx:id="userRowNumberColumn"
															prefWidth="40.0" resizable="false" text="Lp" />
														<TableColumn fx:id="firstNameColumn"
															prefWidth="162.0" resizable="false" text="IMIĘ" />
														<TableColumn fx:id="lastNameColumn" prefWidth="112.0"
															resizable="false" text="NAZWISKO" />
														<TableColumn fx:id="loginColumn" prefWidth="122.0"
															resizable="false" text="LOGIN" />
														<TableColumn fx:id="facultyColumn" prefWidth="122.0"
															resizable="false" text="WYDZIAŁ" />
														<TableColumn fx:id="amountOfIdeasColumn"
															prefWidth="134.0" resizable="false" text="ILOŚĆ POMYSŁÓW" />
													</columns>
													<columnResizePolicy>
														<TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
													</columnResizePolicy>
												</TableView>
											</children>
										</VBox>
									</children>
								</AnchorPane>
							</content>
						</Tab>
						<Tab
							style="-fx-background-color: radial-gradient( focus-angle 0.0deg, focus-distance 0.0%, center 50.0% 50.0%, radius 100.0%, rgb(179,230,230) 0.0, rgb(0,26,128) 100.0 );"
							text="Lista Pomysłów">
							<content>
								<AnchorPane minHeight="0.0" minWidth="0.0"
									prefHeight="180.0" prefWidth="200.0">
									<children>
										<VBox prefHeight="555.0" prefWidth="571.0"
											style="-fx-background-color: radial-gradient( focus-angle 0.0deg, focus-distance 0.0%, center 50.0% 50.0%, radius 100.0%, rgb(204.0,255.0,255.0) 0.0, rgb(128.0,153.0,255.0) 100.0 );"
											AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
											AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
											<children>
												<HBox alignment="CENTER" spacing="10.0">
													<children>
														<Button fx:id="editIdeaButton" mnemonicParsing="false"
															onAction="#handleEditIdea"
															style="-fx-background-color: radial-gradient( focus-angle 0.0deg, focus-distance 0.0%, center 50.0% 50.0%, radius 100.0%, rgb(179,230,230) 0.0, rgb(0,26,128) 100.0 );"
															text="Edytuj Pomysł" />
														<Button fx:id="delIdeaButton" mnemonicParsing="false"
															onAction="#handleRemoveIdea"
															style="-fx-background-color: radial-gradient( focus-angle 0.0deg, focus-distance 0.0%, center 50.0% 50.0%, radius 100.0%, rgb(179,230,230) 0.0, rgb(0,26,128) 100.0 );"
															text="Usuń Pomysł" />
														<Button fx:id="changeIdeaStatusButton" layoutX="203.0"
															layoutY="10.0" mnemonicParsing="false" onAction="#handleChangeIdeaStatus"
															style="-fx-background-color: radial-gradient( focus-angle 0.0deg, focus-distance 0.0%, center 50.0% 50.0%, radius 100.0%, rgb(179,230,230) 0.0, rgb(0,26,128) 100.0 );"
															text="Zmień Status Pomysłu" />
														<ChoiceBox fx:id="statusChoiceBox" prefWidth="150.0"
															style="-fx-background-color: radial-gradient( focus-angle 0.0deg, focus-distance 0.0%, center 50.0% 50.0%, radius 100.0%, rgb(179,230,230) 0.0, rgb(0,26,128) 100.0 );" />
													</children>
													<VBox.margin>
														<Insets bottom="5.0" />
													</VBox.margin>
												</HBox>
												<TableView fx:id="ideasTable" VBox.vgrow="ALWAYS">
													<columns>
														<TableColumn fx:id="ideaRowNumberColumn"
															prefWidth="30.0" text="LP" />
														<TableColumn fx:id="nameColumn" minWidth="45.0"
															prefWidth="56.0" text="NAZWA" />
														<TableColumn fx:id="categoryColumn" prefWidth="85.0"
															text="KATEGORIA" />
														<TableColumn fx:id="addDateColumn" prefWidth="120.0"
															text=" DATA DODANIA" />
														<TableColumn fx:id="editDateColumn" prefWidth="100.0"
															text=" DATA EDYCJI" />
														<TableColumn fx:id="statusColumn" prefWidth="70.0"
															text="STATUS" />
														<TableColumn fx:id="authorColumn" prefWidth="110.0"
															text=" ID PRACOWNIKA" />
													</columns>
												</TableView>
											</children>
										</VBox>
									</children>
								</AnchorPane>
							</content>
						</Tab>
					</tabs>
				</TabPane>
			</center>
			<top>
				<StackPane
					style="-fx-background-color: radial-gradient( focus-angle 0.0deg, focus-distance 0.0%, center 50.0% 50.0%, radius 100.0%, rgb(204.0,255.0,255.0) 0.0, rgb(128.0,153.0,255.0) 100.0 );">
					<children>
						<HBox alignment="CENTER" spacing="10.0">
							<children>
								<Button mnemonicParsing="false" onAction="#handleStatistics"
									style="-fx-background-color: radial-gradient( focus-angle 0.0deg, focus-distance 0.0%, center 50.0% 50.0%, radius 100.0%, rgb(179,230,230) 0.0, rgb(0,26,128) 100.0 );"
									text="Wyświetl Statystyki" textAlignment="CENTER">
									<font>
										<Font name="System Bold" size="12.0" />
									</font>
								</Button>
								<Button layoutX="252.0" layoutY="10.0" mnemonicParsing="false"
									onAction="#handleAddAdmin"
									style="-fx-background-color: radial-gradient( focus-angle 0.0deg, focus-distance 0.0%, center 50.0% 50.0%, radius 100.0%, rgb(179,230,230) 0.0, rgb(0,26,128) 100.0 );"
									text="Dodaj Konto Administratora">
									<font>
										<Font name="System Bold" size="12.0" />
									</font>
								</Button>
								<Button mnemonicParsing="false" onAction="#handleExport"
									style="-fx-background-color: radial-gradient( focus-angle 0.0deg, focus-distance 0.0%, center 50.0% 50.0%, radius 100.0%, rgb(179,230,230) 0.0, rgb(0,26,128) 100.0 );"
									text="Eksportuj Listę Pracowników">
									<font>
										<Font name="System Bold" size="12.0" />
									</font>
								</Button>
							</children>
							<StackPane.margin>
								<Insets bottom="10.0" top="10.0" />
							</StackPane.margin>
						</HBox>
					</children>
				</StackPane>
			</top>
			<bottom>
				<ButtonBar nodeOrientation="RIGHT_TO_LEFT" prefHeight="40.0"
					prefWidth="200.0" BorderPane.alignment="CENTER">
					<buttons>
						<Button mnemonicParsing="false" onAction="#handleLogout"
							style="-fx-background-color: radial-gradient( focus-angle 0.0deg, focus-distance 0.0%, center 50.0% 50.0%, radius 100.0%, rgb(179,230,230) 0.0, rgb(0,26,128) 100.0 );"
							text="Wyloguj" />
						<Button fx:id="changePasswordButton" mnemonicParsing="false"
							onAction="#handleChangePassword"
							style="-fx-background-color: radial-gradient( focus-angle 0.0deg, focus-distance 0.0%, center 50.0% 50.0%, radius 100.0%, rgb(179,230,230) 0.0, rgb(0,26,128) 100.0 );"
							text="Zmień Hasło" textAlignment="CENTER" />
					</buttons>
					<BorderPane.margin>
						<Insets left="35.0" />
					</BorderPane.margin>
					<opaqueInsets>
						<Insets />
					</opaqueInsets>
				</ButtonBar>
			</bottom>
		</BorderPane>
		<ProgressIndicator fx:id="indicator" prefHeight="600.0"
			prefWidth="600.0" AnchorPane.bottomAnchor="0.0"
			AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
			AnchorPane.topAnchor="0.0" />
	</children>
</AnchorPane>
